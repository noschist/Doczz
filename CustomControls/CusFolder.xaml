<UserControl x:Class="Doczz.CustomControls.CusFolder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Doczz.CustomControls"
             mc:Ignorable="d" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             FontFamily="/Doczz;component/Fonts/Raleway/#Raleway" Background="White"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <Viewbox Width="16" Height="16">
            <Canvas Width="24" Height="24">
                <iconPacks:PackIconEvaIcons Kind="Folder" />
            </Canvas>
        </Viewbox>
        <TextBlock Margin="8 0 0 0" x:Name="FolderNameTxt"></TextBlock>
    </StackPanel>
</UserControl>
